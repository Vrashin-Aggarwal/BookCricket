<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10.0dip" />
            <stroke android:width="1.0dip" android:color="#ff20534e" />
            <gradient android:startColor="#ff062d30" android:endColor="#ff4c898e" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="10.0dip" />
            <stroke android:width="1.0dip" android:color="#ff20534e" />
            <solid android:color="#ff125156" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10.0dip" />
            <stroke android:width="1.0dip" android:color="#ff20534e" />
            <gradient android:startColor="#ff4c898e" android:endColor="#ff125156" android:angle="-90.0" />
        </shape>
    </item>
</selector>